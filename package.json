{
  "name": "fakestoreapi-automation",
  "version": "1.0.0",
  "description": "Automated API testing framework for Fake Store API using Jest + Frisby",
  "main": "index.js",
  "scripts": {
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:verbose": "jest --config=jest.config.js --verbose",
    "test:get": "jest --config=jest.config.js tests/get.test.js",
    "test:post": "jest --config=jest.config.js tests/post.test.js",
    "test:put": "jest --config=jest.config.js tests/put.test.js",
    "test:delete": "jest --config=jest.config.js tests/delete.test.js",
    "test:all": "jest --config=jest.config.js --runInBand"
  },
  "keywords": [
    "api-testing",
    "jest",
    "frisby",
    "automation",
    "fake-store-api"
  ],
  "author": "SQE Assignment",
  "license": "MIT",
  "dependencies": {
    "frisby": "^2.1.3",
    "jest": "^29.7.0",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0"
  }
}
